<!doctype html>
<html lang="en">
    <head>
        <!-- Required meta tags -->
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

        {% block styles %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        <title>{{ app_name }}</title>
    </head>
    <body>
        {% block container %}{% endblock %}

        {% block scripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
